<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="Bilten.Domain"
                   namespace="Bilten.Domain">

  <class name="RezultatUkupnoZbirViseKola" table="rezultati_ukupno_zbir_vise_kola" >
    <id name="Id" column="rezultat_id" access="nosetter.camelcase">
      <generator class="identity" />
    </id>
    <property name="RedBroj" column="red_broj" type="Int16" />
    <property name="Rank" column="rank" type="Int16" />
    <property name="TotalPrvoKolo" column="total_1" />
    <property name="TotalDrugoKolo" column="total_2" />
    <property name="TotalTreceKolo" column="total_3" />
    <property name="TotalCetvrtoKolo" column="total_4" />
    <property name="Total" column="total" />
    <many-to-one name="Gimnasticar" class="GimnasticarUcesnik" column="gimnasticar_id" />
  </class>

</hibernate-mapping>
